<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC   
"-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gos.dao.DvdDAO">
	
	<select id="queryAll" resultType="Dvd">
		select * from t_dvd;
	</select>
	
	<select id="query" parameterType="int" resultType="Dvd">
		select * from t_dvd where id=#{id}
	</select>
		
</mapper>




























